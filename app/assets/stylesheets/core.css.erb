/* welcome popup */

#popup.welcome-tour {
  top: 12px;
  width: 560px;
  box-shadow: -0.5em 0.5em 0 rgba(0,0,0,0.3);
  padding:0;
}

#popup.welcome-tour .welcome-content {
  background-color: #F27820;
  padding: 0 5px 5px 5px;
}

#popup.welcome-tour .welcome-inner {
  background-color: #FDE4D2;
  color: #000;
  padding: 1em 2em;
}

#popup.welcome-tour .close {
  right: 0.5em;
  top: 1.5em;
}

#popup.welcome-tour h2 {
  font-size: 2em;
  font-weight: normal;
  text-transform:uppercase;
  padding:0.2em 0 0 0.5em;
  color:#000;
}

