}

.england .devolved-header {
  padding:0 0 0 18px;
  background: url(<%= asset_path "flags.png" %>) no-repeat scroll 0 -17px #fff;
}
.scotland .devolved-header {
  padding:0 0 0 18px;
  background: url(<%= asset_path "flags.png" %>) no-repeat scroll 0 -60px #fff;
}

.wales .devolved-header {
  padding:0 0 0 18px;
  background: url(<%= asset_path "flags.png" %>) no-repeat scroll 0 3px #fff;
}
.northern-ireland .devolved-header {
  /* don't show a flag for NI */
  padding: 0;
  background: none;
}

/* Error messages */

.error-notification,
.error-area{
  border:1px solid #ffc946;
  background-color:#fbedcd;
  padding: 0 1em;
}
.error-notification p,
.error-area p{
  font-size: 1em;
}
.error-area span{
  display:block;
