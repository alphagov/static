html, body, div, h1, h2, h3, h4, h5, h6, article, aside, footer, header, hgroup, nav, section {
  margin: 0;
  padding: 0;
  vertical-align: baseline;
}

/* self-clearing floats */
.group:before, .group:after { content: "\0020"; display: block; height: 0; overflow: hidden; }
.group:after { clear: both; }
/* Fix clearfix */
.group { zoom: 1; }

/*
 * Core global styles
 */

html {
  background: #C9D0D0;
}

body {
  color: #333;
  font-family: "NTA", "Helvetica Neue", Arial, Helvetica, sans-serif;
  line-height: 1.5em;
  font-weight:300;
}
#wrapper {
  background-color: #F2F2F2;
}
#wrapper.business header.page-header hgroup{
  background-image:url(<%= asset_path "business_link_logo.png" %>);
  background-position:97% 1em;
  background-repeat:no-repeat;
}
#footer{
  background-color: #C9D0D0;
}
.inner {
  margin: 0 auto;
  padding: 1em;
  position: relative;
}

section#content {
  margin: 0 auto;
  max-width: 960px;
  position: relative;
}
.lte-ie8 section#content {
  _width: 960px;
}

ol, ul, nav ol, nav ul {
  list-style: inherit;
}

fieldset {
  border: none;
  padding: 0;
}

h2 {
  font-size: 112.5%;
  line-height: 1.18em;
  margin-top: 0.5em;
  margin-bottom: 0.25em;
}

h3 {
  font-size: 1.2em;
  line-height: 1.11em;
  margin-top: 0.5em;
  margin-bottom: 0.25em;
}

h4 {
  margin-top: 0.5em;
  margin-bottom: 0.25em;
}

/*
 * Global link styles
 */

a,
a:visited {
  color: #2E3191;
}

a:hover {
  color: #2E8ACA;
}

a:active {
  color: #c11;
}

a[rel="external"] {
  display: inline-block;
}

a[rel="external"]:after {
  content: url(<%= asset_path "external-link.png" %>);
  margin-right: 0.2em;
  margin-left: -0.2em
}

/*
 * Locate me styles
 */

.popover-mask {
  background-color: rgba(0,0,0,0.5);
  display: none;
  height: 100%;
  left: 0;
  margin: 0;
  top: 30px;
  padding: 0;
  position: fixed;
  width: 100%;
  z-index: 60;
}

.popover-mask .popover {
  background: #fafafa;
  border: solid 2px #bbb;
  -moz-border-radius: 0.5em;
  -webkit-border-radius: 0.5em;
  border-radius: 0.5em;
  box-shadow: 4px 4px 10px rgba(0,0,0,0.3);
  margin: 4em auto 0 auto;
  padding: 0 2.5em 2em 2.5em;
  position: relative;
  min-width: 25em;
}

.popover .close {
  position: absolute;
  top: -19px;
  right: -19px;
}

.popover h2 {
  margin: 0.5em 0;
  padding-bottom: 0.2em;
}

#global-locator-form label,
#global-locator-form .ask_location{
  display: inline;
}
#global-locator-form .ask_location.hidden { display: none; }
#global-locator-form .found_location p,
  #global-locator-form .finding_location p {
    margin: 0em;
  }

/*
 * Related box
 * this related stuff needs re-examining - might be a bit over the top
 */

.related-positioning {
  height: 0;
  left: 0;
  position: fixed;
  top: 8em;
  width: 100%;
}
.lte-ie8 .related-positioning {
  _position: absolute !important;
  z-index: 10;
}

#wrapper.programme .related-positioning {
  /*  top: 12.1em;*/
}

.related-container {
  height: 0;
  margin: 0 auto;
  max-width: 960px;
  position: relative;
  z-index: 50;
}
.lte-ie8 .related-container {
  _width: 960px;
}

.related {
  position: absolute;
  right: 0;
  width: 15em;
}
.lte-ie8 .related {
  display: inline;
  height: 1%;
  zoom: 1;
}

.related .inner {
  background-color: #E5E5E5;
  padding: 0 1em 0.25em 1.2em;
  width: auto;
  border-left:1px solid #C9D0D0;
}
.related h2 {
  color: #000;
  font-weight:300;
  height: 30px;
  line-height: 1.5em;
  margin: 0 0 0 -21px;
  padding: 30px 0 0 20px;
  position: relative;
  top: -13px;
}

.related ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.related li {
  padding-bottom: 0.45em;
  padding-left:0;
}

#wrapper .related li a {
  background-position: 0 -287px;
  background-repeat: no-repeat;
  display: block;
  line-height:1.25em;
  padding: 0em 0 0.5em 0em;
  orphans: 2;
}
#wrapper .related li a:hover {
  background-position: 0 0;
}

.lte-ie8 #wrapper .related li a {
  height: 1%;
}

.related li a[href*="#"] {
  /* grey out stub links */
  background: none;
  color: #888;
  cursor: default;
  text-decoration: none;
}

.related li.related-topic {
  border-top: 1px solid #C9D0D0;
  list-style: none outside none;
  margin: 1em 0 0;
  padding: 1em 0;
}

.related li.related-topic span {
  font-weight: bold;
}

#wrapper .related li.related-topic a {
  padding-left: 0;
}

.related li.related-topic span.arrow {
  display: none;
}

#wrapper .related.sections-list li a {
  padding: 0.1em 0em;
}

/*
  page headers
*/

header.page-header hgroup {
  background-color: #fff;
  display: block;
  margin: 0 15em 0 0;
  padding: 0.2em 1em 1.5em 0.5em;
}


header.page-header hgroup h1 {
  background-repeat: no-repeat;
  color: #000;
  font-family:"NTA", "Helvetica Neue", Arial, Helvetica, sans-serif;
  font-size: 2.75em;
  font-weight:400;
  letter-spacing: 0em;
  line-height: 1.041666667em;
  padding: 0.3em 1.5em 0.35em 1.65em;
}
.business header.page-header hgroup h1 {
    padding-right:3em;
  }

.lte-ie8 header.page-header hgroup h1 {
  height: 1%;
}

.service header.page-header hgroup h1 {
  background-position: 0 12px;
}
.guide header.page-header hgroup h1 {
  background-position: 15px 14px;
}
.answer header.page-header hgroup h1 {
  background-position: 12px 14px;
}
.programme header.page-header hgroup h1 {
  background-position: 0 14px;
}
.licence header.page-header hgroup h1 {
  background-position: 16px 0;
}

header.page-header hgroup h1 span {
  display: block;
  font-size: 0.4319em;
  line-height: 1em;
  margin-bottom: 0.5em;
  text-shadow: none;
  font-weight:300;
}

header.page-header hgroup h2 {
  color: #666;
  font-size: 1em;
  margin: 0.75em 0 0 4.5em;
}

/*
  ============================== Stuff (mainly) used in publications =============================
*/

/* Programmes */

#wrapper.programme header.page-header hgroup h1 {
  background-image: url(<%= asset_path "icon-programme-l.png" %>);
}

/* Services */

#wrapper.service header.page-header hgroup {
  /*border-color: #2DA096;*/
}

#wrapper.service header.page-header hgroup h1 {
  background-image: url(<%= asset_path "icon-service-l.png" %>);
}

/* Quick answers */

