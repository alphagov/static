  font-size: 1.3em;
  font-weight: bold;
  margin: 0.5em 0 0.3em 0.4em;
  padding: 0.5em 1em;
  text-align: center;
  background: #666;
}

#feedback-cta #cta-yes{
  background: #D33883;
}

#feedback-cta .sets-cookie {
  background: none;
  margin: 1em 0 0 0;
  padding: 0.25em 0 0 0;
}
#feedback-cta .sets-cookie a {
  color: inherit;
  font-size:0.85em;
  padding:0 0 0 0.5em;
}

#popup .vcard span{
  display:block;
}

#popup ul{
  margin:0 0 0 5em;
  padding:0;
}
#related-popup h2{
  padding:0;
}
#feedback-mechanism{
  background-color:#EEE;
  padding:1em;

}
#feedback-options select,
#feedback-mechanism textarea{
  width:90%;
  display:block;
  margin:0.5em 0;
  padding: 0.4em;
}
.feedback-tools .feedback-thanks{
  font-size:1.7em;
}
#related-popup{
  padding:1em;
  margin:1em;
  border-top:1px solid #eee
}
#related-popup ul{
  list-style-type:none;
  margin:0;
  padding:0;
}
#related-popup ul li{
  display:inline;
  padding:0;
  margin:0;
  margin-right:1em;
}

/*shared bits for buttons on a, button and input */
input, button {
  font-family:"NTA", "Helvetica Neue", Arial, Helvetica, sans-serif;
}

.button .call-to-action,
.button.big,
.button,
.button:visited {
  background-color:#28a198;
  color:#fff;
   -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow:0 4px 0 #1C7568, 0 8px 10px rgba(0,0,0,.05);
  -moz-box-shadow:0 4px 0 #1C7568, 0 8px 10px rgba(0,0,0,.05);
  box-shadow:0 3px 0 #1C7568, 0 8px 10px rgba(0, 0, 0, 0.05);
  font-weight:300;
}

.button:hover span.call-to-action,
.button.big:hover,
.button:hover {
  background-color: #3ABAA8;
  color: #fff;
}

.button:active,
.button.big:active,
.button:active span.call-to-action {
  -webkit-transform: translate(0, 4px);
  -moz-transform: translate(0, 4px);
  -o-transform: translate(0, 4px);
  transform: translate(0, 4px);
  box-shadow:none;
  outline:none;
}

.get-started .button.big:active{
   -webkit-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0);
  box-shadow:none;
  outline:none;
}

/* buttons! */
.button,
.button:visited {
  display:inline;
  line-height:0.8em;
  border:none;
  text-decoration: none;
  position:relative;
  top:-3px;
  line-height:1.3em;
}

.button:focus span,
.button:active span
.button:active,
.button:focus{
  outline:none;
  background-color:transparent;
  color: #fff;
}
.button:active,
.button:focus{
  background-color:#28A197;
}

.lte-ie .button:active {
  /*top: 1px;*/
}
.button:focus {
  /*background-color: #001950;*/
}

.button,
.button.medium {
  padding: 0.3em 0.5em 0.1em 0.5em;
}

.button.medium {
  font-size: 1.25em;
}


/* get started buttons */
.get-started {
  text-align:right;
  margin:1.5em 0 0 0;
}

.get-started .button {
  text-decoration:none;
  background-color: transparent;
  box-shadow:none;
  outline:none;
}
.lte-ie8 .get-started .button {
  _width: 60%;
}

.get-started .button{
  text-align:left;
}
.get-started .button .destination {
  font-size:0.7368em;
  color:#333;
  margin-top:1em;
  display: block;
}

.get-started .button .call-to-action,
.button.big {
  display:block;
  font-size:1.3162em;
  padding:0.5em 1.7em 0.35em 0.5em;
