}

/* feedback popup */

#feedback-cta{
  background-color: #333;
  color: white;
  box-shadow: -0.5em 0.5em 0 rgba(0,0,0,0.3);
  z-index:9999;
}
.lte-ie8 #feedback-cta {
  position: absolute;
}

#feedback-cta .inner {
  padding: 0;
  position: relative;
}

#feedback-cta .inner .close {
  right: -1em;
  top: -1em;
}
.lte-ie8 #feedback-cta .inner .close {
  right: 0em;
  top: -0.5em;
}

#feedback-cta.left {
  position: fixed;
  left: -0.5em;
  display: block;
  bottom: 5%;
  width: 12em;
  min-height:10em;
  padding:0em 0.5em 0.5em 1em;
  -webkit-transition: left 0.5s ease-in-out;
  -moz-transition: left 0.5s ease-in-out;
}
.lte-ie8 #feedback-cta.left {
  display:none;
}



#feedback-cta.hide:hover,
#feedback-cta.hide:active,
#feedback-cta.hide:focus{
  left:0px;
}
#feedback-cta.hide{
  left:-12em;
}

#feedback-cta h2{
  font-size: 0.85em;
  font-weight: normal;
  letter-spacing: 0.15em;
  margin: 0.4em -2px 0.75em 0;
  width:10px;
  text-transform: uppercase;
  /*writing-mode:tb-rl;*/
	-webkit-transform:rotate(90deg);
	-moz-transform:rotate(90deg);
	-o-transform: rotate(90deg);
  transform: rotate(90deg);
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=1);
  display:absolute;
  float:right;
  color:#B4892D;
}

#feedback-cta p{
  line-height: 1.2;
  padding:0.5em;
