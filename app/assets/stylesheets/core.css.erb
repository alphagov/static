  margin: 1em 0;
  padding: 0 1em 1em 1em;
}

.devolved-header {
  background-color: #fff;
  border-bottom: 1px solid #ccc;
  font-size: 0.85em;
}
.devolved-header a {
  -moz-border-radius: 0.25em;
  -webkit-border-radius: 0.25em;
  border-radius: 0.25em;
  font-size: 0.75em;
  background-color: #92AD54;
  color: #fff;
  margin-left: 0.5em;
  padding: 0.25em 0.5em;
