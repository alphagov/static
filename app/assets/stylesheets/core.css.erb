}

/* Guides */

#wrapper.guide header.page-header hgroup h1 {
  background-image: url(<%= asset_path "icon-guide-l.png" %>);
}

/* articles */

article {
  background: #fff;
  float: left;
  margin-right: 12em;
  min-height: 30em;
  position: relative;
  width: 100%;
  z-index: 1;
}
.lte-ie8 article {
  float: none;
  _height: 30em;
}

.article-container {
  background-color: #fff;
  margin-right: 15em;
}

.article-container article {
  margin-right: 0;
}

article .inner {
  padding: 0 5em 2em 5em;
  width: auto;
}

/* Meta-data */
.meta-data {
  color: #333;
  float: left;
  padding: 2em 0 0 3em;
}
.lte-ie8 .meta-data {
  *float: none;
}

.meta-data .inner {
  padding: 0 2em 0 2em;
}

.print-and-modified-date {
  float: left;
}

.print-link {
  float: right;
}

.print-link a, .print-link a:link, .print-link a:visited {
  background: url(<%= asset_path "print.png" %>) no-repeat 100% 50%;
  display: inline-block;
  padding-left: 30px;
  padding-right:2em;
  padding-top:4px;
}

.modified-date {
  float: left;
}

/*
 * auto complete
 * borrowed from the alpha
 */
.hint-suggest{
  border: 1px solid #EEEEEE;
  margin:0;
  background-color:#fff;
  display: block;
  left: 0;
  padding: 0.5em;
  position: relative;
  top: 0;
  width: 163px;
  color:#666;
  font-size:0.95em;
}

.search-loading{
  background: url(<%= asset_path "ajax-loader.gif" %>) no-repeat 4px 50% #fff;
  padding-left:30px;
  width: 133px;
}
#main_autocomplete {
  color: inherit;
}

ul.ui-menu {
  padding: 0;
}
ul.ui-widget {
  font-size: 1em;
}
ul.ui-autocomplete {
  background: none repeat scroll 0 0 #fff;
  border: 1px solid #8F8F8F;
  list-style-type:none;
  padding:0;
  margin:0;
  margin-top:1px;
  position:absolute;
  box-shadow: 0 2px 3px rgba(0, 0, 0, 0.25);
  top: 1.8em;
  left: 0;
  display: none;
}

ul.ui-autocomplete li {
  border-top: 1px solid #eee;
  list-style-type:none;
  margin:0;
}
ul.ui-autocomplete.ui-menu .ui-menu-item a,
ul.ui-autocomplete .search-site a {
  padding: 0.2em 1em;
  display:block;
  color: #333;
  text-decoration:none;
}

ul.ui-autocomplete .search-site a,
ul.ui-autocomplete .search-site a:hover {
  border-top:3px solid #dedede;
  background: url(<%= asset_path "search.png" %>) no-repeat 12px 50% #eee;
  padding-left: 3em;
}
ul.ui-autocomplete li a em {
  font-style: normal;
  font-weight: bold;
  padding: 0.125em;
}
ul.ui-menu .ui-menu-item a.ui-state-hover,
ul.ui-menu .ui-menu-item a.ui-state-active,
ul.ui-autocomplete .search-site a:hover {
  margin: 0;
  background-color:#ffffcc;
}
ul.ui-autocomplete li a,
ul.ui-autocomplete li a:hover {
  background-position: 8px 50%;
}

ul.ui-autocomplete.ui-menu .ui-menu-item.search-site em {
  font-style: italic;
}
ul.ui-corner-all, ul li a.ui-corner-all {
  border-radius: 0 0 0 0;
}

/* Location in header bar */
#location-set-message.removing,
#location-unset-message.removing {
  display:none;
}

#location-set-message,
#location-unset-message{
  background: url(<%= asset_path "location-pin_outline_tiny.png" %>) no-repeat 0 50%;
  position: fixed;
  top: 0;
  left: 20%;
  display: block;
  z-index: 100000;
  line-height: 3.5em;
  padding: 0 0 0 1.5em;
  margin: 0;
  color: #bbb;
  font-size: 0.75em;
}
.lte-ie8 #location-set-message,
.lte-ie8 #location-unset-message, {
  display: none;
}

#location-set-message a,
#location-unset-message a {
  color: #bbb;
  color: #d8d8d8;
}
#location-set-message small,
#location-unset-message small {
  font-size: inherit;
}

/*
 * Devolved content
 */

.devolved-content {
  background: none repeat scroll 0 0 #fff;
